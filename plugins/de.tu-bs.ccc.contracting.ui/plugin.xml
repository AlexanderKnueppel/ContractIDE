<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.newWizards">
      <category
            id="de.tu-bs.ccc.contracting.ui.wizards"
            name="ContractIDE">
      </category>
      <wizard
            category="de.tu-bs.ccc.contracting.ui.wizards"
            class="de.tu_bs.ccc.contracting.ui.wizards.CCCProjectNewWizard"
            icon="icons/cide2_16.png"
            id="de.tu-bs.ccc.contracting.ui.wizards.new.custom"
            name="ContractIDE Project">
      </wizard>
      <wizard
            category="de.tu-bs.ccc.contracting.ui.wizards"
            class="de.tu_bs.ccc.contracting.ui.wizards.CompoundWizard"
            icon="icons/compound_64.png"
            id="contractmodelling.wizards.CompoundWizard"
            name="Compound">
      </wizard>
      <wizard
            category="de.tu-bs.ccc.contracting.ui.wizards"
            class="de.tu_bs.ccc.contracting.ui.wizards.ComponentWizard"
            icon="icons/atomic_component_64.png"
            id="contractmodelling.wizards.ComponentWizard"
            name="Atomic Component">
      </wizard>
   </extension>
   
	<extension point="org.eclipse.ui.menus">
	  <menuContribution  allPopups="false" locationURI="popup:org.eclipse.ui.popup.any?after=additions">
         <menu
               id="de.tu-bs.ccc.contracting.ui.CCCMenu"
               label="ContractIDE"
               icon="icons/cide2_16.png">
            <command
                  commandId="de.tu-bs.ccc.contracting.ui.exporter_command"
                  label="Export to CCC config. (.xml)"
                  style="push">
            </command>
         </menu>
	  </menuContribution>
	</extension>
 <extension
       point="org.eclipse.ui.commands">
    <command
          defaultHandler="de.tu_bs.ccc.contracting.ui.handler.ExportHandler"
          id="de.tu-bs.ccc.contracting.ui.exporter_command"
          name="ExportToXML">
    </command>
 </extension>
   
</plugin>
